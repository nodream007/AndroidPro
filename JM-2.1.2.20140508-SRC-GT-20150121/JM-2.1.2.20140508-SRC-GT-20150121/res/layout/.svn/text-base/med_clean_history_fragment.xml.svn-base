<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/histroy_linear"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/list_box_left1"
    android:orientation="vertical" >

    <LinearLayout
        android:id="@+id/history_date_picker"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="20dp"
        android:gravity="center_vertical" >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="24sp"
            android:text="诊疗时间:从" />

        <TextView
            android:id="@+id/send_med_start_time_in_history"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:background="@drawable/bg_with_corner_5dp_blue"
            android:gravity="center"
            android:textSize="@dimen/middle_text_size" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:textSize="24sp"
            android:text="至" />

        <TextView
            android:id="@+id/send_med_end_time_in_history"
            android:layout_width="150dp"
            android:layout_height="40dp"
            android:gravity="center"
            android:textSize="@dimen/middle_text_size"
            android:background="@drawable/bg_with_corner_5dp_blue"
            android:layout_marginLeft="10dp" />
    </LinearLayout>

    <ListView
        android:id="@+id/send_med_clean_history_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginBottom="10dp"
         >
    </ListView>

</LinearLayout>