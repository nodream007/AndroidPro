package com.ceres.jailmon.data;

import java.util.List;

public class MedicinePatientInfo {
	private String type;
	private String doctor;
	private String time;
	private String medicineNotice;
	private String remark;
	private List<MedicinePatientMedicineInfo> medicineInfoList;
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public String getDoctor() {
		return doctor;
	}
	public void setDoctor(String doctor) {
		this.doctor = doctor;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time = time;
	}
	public String getMedicineNotice() {
		return medicineNotice;
	}
	public void setMedicineNotice(String medicineNotice) {
		this.medicineNotice = medicineNotice;
	}
	public String getRemark() {
		return remark;
	}
	public void setRemark(String remark) {
		this.remark = remark;
	}
	public List<MedicinePatientMedicineInfo> getMedicineInfoList() {
		return medicineInfoList;
	}
	public void setMedicineInfoList(
			List<MedicinePatientMedicineInfo> medicineInfoList) {
		this.medicineInfoList = medicineInfoList;
	}
	
}
